___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const logToConsole = require('logToConsole');
const sendPixel = require('sendPixel');
const encodeUriComponent = require('encodeUriComponent');

let id = data.juc_ID || '';
let value = data.juc_VALUE || '0';

let commission = data.juc_COMMISSION || '0';
if (data.juc_COMMISSION_IS_PERCENTAGE) commission+='%';

let param = data.juc_PARAM || '';

let eps=[];
let extended_parameters = data.juc_EXTENDED_PARAMETERS || [];
for (let i in extended_parameters) {
  let t=extended_parameters[i];
  for (let j in t) {
    eps.push(t[j]);
    if (eps.length>14) break;
  }
  if (eps.length>14) break;
}

let query={
	id: id,
  	val: value,
  	comm: commission,
  	param: param,
};
if (eps.length) {
  for (let i=0;i<eps.length;i++) {
    query['ep'+(i<10?'0':'')+i]=eps[i];
  }
}

let q=[];
for (let k in query) {
  q.push(encodeUriComponent(k)+'='+encodeUriComponent(query[k]));
}

sendPixel('https://www.joturl.com/c/?'+q.join('&'), data.gtmOnSuccess, data.gtmOnFailure);


